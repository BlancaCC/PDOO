            ///~~~~~~~~~~~~~~~~ Weapon Type ~~~~~~~~~~~

            enum WeaponType { 
                LASER(2.0), MISSILE(3.0), PLASMA(4.0); 

                private float power; 

                //constructor 
                WeaponType ( float power){
                    this.power = power; 
                }

                // Consultor
                 float getPower(){
                     return power; 
                 }

            } //enum WeaponType

